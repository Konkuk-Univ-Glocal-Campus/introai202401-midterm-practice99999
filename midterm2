    "num_samples = 10    #데이터셋의 구성을 확인하기 위한 샘플 개수\n",
    "interval = len(data_test) // num_samples\n",
    "\n",
    "# 이미지 시각화\n",
    "figure = plt.figure(figsize=(12, 4))    #이미지를 표현할 figure 객체 생성 후 그림의 크기 설정 \n",
    "cols, rows = 5, 2   #2개의 행에 이미지 5개씩 표현\n",
    "\n",
    "for i in range(num_samples):\n",
    "    sample_idx = i * interval  # 일정한 간격으로 샘플 선택을 위한 연산\n",
    "    img, label = data_test[sample_idx]  # 선택된 샘플의 이미지와 레이블 저장\n",
    "    figure.add_subplot(rows, cols, i + 1)  # 이미지 배치(subplot의 위치 결정)\n",
    "    plt.title(data_test.classes[label])  # 클래스명(패션 아이템 이름)으로 타이틀 설정\n",
    "    plt.axis(\"off\")  # 좌표축과 선을 표시하지 않기 위한 설정\n",
    "    plt.imshow(img.squeeze(), cmap=\"gray\")  # 그레이스케일 이미지로 출력\n",
    "\n",
    "plt.show()\n"
